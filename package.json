{
  "name": "docmq",
  "description": "A document based messaging queue for Mongo, DocumentDB, and others",
  "version": "0.3.0",
  "author": {
    "name": "Jakob Heuser",
    "email": "oss@codedrift.com"
  },
  "repository": "jakobo/docmq.git",
  "scripts": {
    "bench": "node --loader=ts-node/esm ./scripts/run-benchmark.ts",
    "build": "run-s unbuild 'verify:*'",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "dev": "run-s watch",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "rel": "release-it",
    "release:before:init": "run-s clean build test",
    "rev": "release-it --preRelease",
    "syncpack": "syncpack",
    "test": "ava",
    "unbuild": "unbuild",
    "verify:cjs": "cd verify/cjs && npm install && node cjs.verify.js",
    "verify:esm": "cd verify/esm && npm install && node esm.verify.js"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "cron-parser": "^4.4.0",
    "lokijs": "^1.5.12",
    "luxon": "^2.4.0",
    "serialize-error": "^11.0.0",
    "tslib": "^2.4.0",
    "typed-emitter": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/benchmark": "^2",
    "@types/eslint": "*",
    "@types/lokijs": "^1.5.7",
    "@types/luxon": "^2",
    "@types/node": "^17.0.41",
    "@types/pg": "^8",
    "@types/uuid": "^8",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "ava": "^4.3.0",
    "benchmark": "^2.1.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "esm-multi-loader": "^1.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "madge": "^5.0.1",
    "mongodb": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.0",
    "pg": "^8.7.3",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "release-it": "*",
    "shx": "^0.3.4",
    "syncpack": "^7.2.1",
    "testdouble": "^3.16.6",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "unbuild": "latest"
  },
  "peerDependencies": {
    "mongodb": "^4.7.0",
    "pg": "^8.7.3"
  },
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "file": "./src/index.ts"
    },
    "./driver/mongo": {
      "types": "./dist/mongo.d.ts",
      "import": "./dist/mongo.mjs",
      "require": "./dist/mongo.cjs",
      "file": "./src/driver/mongo.ts"
    },
    "./driver/loki": {
      "types": "./dist/loki.d.ts",
      "import": "./dist/loki.mjs",
      "require": "./dist/loki.cjs",
      "file": "./src/driver/loki.ts"
    },
    "./driver/postgres": {
      "types": "./dist/postgres.d.ts",
      "import": "./dist/postgres.mjs",
      "require": "./dist/postgres.cjs",
      "file": "./src/driver/postgres.ts"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "14.19.0"
  }
}
